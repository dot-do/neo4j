{
  "name": "neo4j.do",
  "version": "0.1.0",
  "description": "Neo4j-compatible graph database on Cloudflare Workers",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "wrangler dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "deploy": "wrangler deploy"
  },
  "keywords": [
    "neo4j",
    "graph-database",
    "cypher",
    "cloudflare-workers",
    "d1",
    "sqlite"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/neo4j.do"
  },
  "homepage": "https://neo4j.do",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241230.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "better-sqlite3": "^12.5.0",
    "neo4j-driver": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^3.99.0"
  }
}
